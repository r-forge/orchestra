plugin.com.addictedtor.jedit.R.RPlugin.name=R
plugin.com.addictedtor.jedit.R.RPlugin.author=Romain Francois
plugin.com.addictedtor.jedit.R.RPlugin.version=0.0
plugin.com.addictedtor.jedit.R.RPlugin.activate=startup
plugin.com.addictedtor.jedit.R.RPlugin.usePluginHome=true
