plugin.com.addictedtor.jedit.R.RPlugin.depend.0=jdk 1.5
plugin.com.addictedtor.jedit.R.RPlugin.depend.1=jedit 04.03.17.00
plugin.com.addictedtor.jedit.R.RPlugin.depend.2=plugin console.ConsolePlugin 4.3
plugin.com.addictedtor.jedit.R.RPlugin.depend.3=plugin errorlist.ErrorListPlugin 1.7
plugin.com.addictedtor.jedit.R.RPlugin.depend.4=plugin sidekick.SideKickPlugin 0.7.5

